{
    "name": "vallarj/php-json-api",
    "description": "JSON API library for PHP",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "doctrine/inflector": "^1.3",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Vallarj\\JsonApi\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "Vallarj\\JsonApi"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
